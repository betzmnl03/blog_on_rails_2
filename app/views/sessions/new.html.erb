<h1 class="m-4">Sign in</h1>
<div class="container m-4 p-4 border col-sm-6">
<%= form_with url: session_path, :class=>"form-group", local: true do |f|  %>
    <%= f.label :email %>
<div class="input-group mb-2">
    <%= f.email_field :email%>
</div>
<%= f.label :password  %>
<div class="input-group mb-2">     
    <%= f.password_field :password  %> 
</div>
<%= f.submit "Sign In", :class => 'btn btn-outline-primary mt-4'%> 
<% end %> 
</div>